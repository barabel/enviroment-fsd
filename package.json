  "scripts": {
    "local_server": "node server.js",
    "watch": "gulp --developer --openssl-legacy-provider",
    "dev": "gulp --developer --openssl-legacy-provider",
    "prodwatch": "gulp --openssl-legacy-provider",
    "devbuild": "gulp build --developer --openssl-legacy-provider",
    "build": "gulp  build --openssl-legacy-provider",
    "build:bx": "gulp build:bx --openssl-legacy-provider && cp -R ./dist/assets/* ./htdocs/assets",
