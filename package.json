    "deploy:prod": "gulp deploy",
    "twigtojs": "node ./commands/twigtojs/index.js",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "buildCheck": "gulp build --check --openssl-legacy-provider",
    "prepare": "husky install"
